<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.cordovaLearn" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cordovaLearn</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <platform name="android">
    <icon src="Resources/icon/drawable-ldpi-icon.pngs" density="ldpi"/>
    <icon src="Resources/icon/drawable-mdpi-icon.png" density="mdpi"/>
    <icon src="Resources/icon/drawable-hdpi-icon.png" density="hdpi"/>
    <icon src="Resources/icon/drawable-xhdpi-icon.png" density="xhdpi"/>
    <icon src="Resources/icon/drawable-xxhdpi-icon.png" density="xxhdpi"/>
    <icon src="Resources/icon/drawable-xxxhdpi-icon.png" density="xxxhdpi"/>
    <splash src="Resources/splash/drawable-land-ldpi-screen.png" density="land-ldpi"/>
    <splash src="Resources/splash/drawable-land-mdpi-screen.png" density="land-mdpi"/>
    <splash src="Resources/splash/drawable-land-hdpi-screen.png" density="land-hdpi"/>
    <splash src="Resources/splash/drawable-land-xhdpi-screen.png" density="land-xhdpi"/>
    <splash src="Resources/splash/drawable-land-xxhdpi-screen.png" density="land-xxhdpi"/>
    <splash src="Resources/splash/drawable-land-xxxhdpi-screen.png" density="land-xxxhdpi"/>
    <splash src="Resources/splash/drawable-port-ldpi-screen.png" density="port-ldpi"/>
    <splash src="Resources/splash/drawable-port-mdpi-screen.png" density="port-mdpi"/>
    <splash src="Resources/splash/drawable-port-hdpi-screen.png" density="port-hdpi"/>
    <splash src="Resources/splash/drawable-port-xhdpi-screen.png" density="port-xhdpi"/>
    <splash src="Resources/splash/drawable-port-xxhdpi-screen.png" density="port-xxhdpi"/>
    <splash src="Resources/splash/drawable-port-xxxhdpi-screen.png" density="port-xxxhdpi"/>
  </platform>
</widget>
